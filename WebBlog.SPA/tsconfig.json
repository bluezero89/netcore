{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": ".",
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "sourceMap": true,
        "skipDefaultLibCheck": true,
        "strict": true,
        "types": ["webpack-env"]
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "bin",
        "node_modules"
    ]
}
  